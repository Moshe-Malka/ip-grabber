{
    "manifest_version": 2, 
    "name": "ip-grabber", 
    "version": "0.1.0", 
    "permissions": ["<all_urls>"],
    "icons":{
        "16": "ip_16.png",
        "48": "ip_48.png",
        "128": "ip_128.png"
    },
    "browser_action":{
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }